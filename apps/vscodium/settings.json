{
  "workbench.colorTheme": "Visual Studio Light",
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1,
  },
  "editor.formatOnSave": true,
  "editor.aiStats.enabled": true,
  "editor.autoIndentOnPaste": true,
  "editor.definitionLinkOpensInPeek": true,
  "terminal.external.osxExec": "",
  "terminal.integrated.enableImages": true,
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "editor.fontFamily": "MesloLGS NF, Menlo, Monaco, 'Courier New', monospace",
  "workbench.startupEditor": "none",
  "roo-cline.allowedCommands": [
    "git log",
    "git diff",
    "git show",
    "ls",
    "less",
    "tail",
    "cat",
    "echo",
    "stat",
    "cd",
    "git status",
    "npm test",
    "pytest",
    "git worktree",
    "git branch",
    "git bisect",
    "pylint",
    "vale",
    "mypy",
    "grep",
    "wc",
    "head",
    "pip --version",
    "pip-compile",
    "./scripts/update_requirements.sh",
    "npx eslint",
    "npx stylelint",
    "python -m pytest",
    "python scripts/download_external_media.py",
    "npm view",
    "npm list",
    "pip show",
    "alt-text-llm",
    "git reflog",
    "npm run test:a11y",
    "npx tsc",
    "which",
    "pnpm --version",
    "pnpm build",
    "uv lock",
    "uv sync",
    "pnpm list",
    "pnpm test",
    "pnpm eslint",
    "pnpm playwright --version",
    "pnpm why",
    "pnpm view",
    "file",
    "git submodule add",
    "git config user.name",
    "git config user.email",
    "sort",
    "uv run pytest",
    "magick",
    "sed",
    "pnpm -s test",
    "true",
    "git checkout",
    "test",
    "uv --version",
    "git stash list",
    "python3 scripts/run_push_checks.py",
    "pnpm check",
    "pnpm tsc",
    "df",
    "brew info",
    "gh pr view",
    "deepsource issues",
    "deepsource",
    "pnpm exec playwright",
    "chmod +x",
    "ots",
    "git add",
    "git commit",
    "readlink",
    "rg",
    "llm models",
    "llm plugins",
    "find",
    "git remote add",
    "git ls-tree",
    "awk",
    "npm run typecheck",
    "pnpm install"
  ],
  "roo-cline.deniedCommands": [],
  "git.openRepositoryInParentFolders": "always",
  "pylint.args": [""],
  "markdownlint.config": {},
  "terminal.integrated.inheritEnv": false,
  "security.workspace.trust.untrustedFiles": "open",
  "playwright.reuseBrowser": false,
  "playwright.showTrace": false,
  "playwright.runGlobalSetupOnEachRun": false,
  "roo-cline.debug": false,
}
