[user] 
	name = Alex Turner
	email = alex@turntrout.com
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[core]
	autocrlf = input
	safecrlf = true
	hooksPath = ~/.git_hooks
[alias]
	co = checkout
	ci = commit
	st = status
	br = branch
	hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
	type = cat-file -t
	dump = cat-file -p
[diff]
	tool = vscode
[credential]
	helper = store
[init]
	defaultBranch = main
